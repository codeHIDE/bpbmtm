			e.printStackTrace();
		}
		if ("".equals(checkCode)) {
			checkCode = "3001";
		}
		settBank.setRespCode(checkCode);
		settBank.setRespDesc(ValueTool.SYS_CODE.get(checkCode));
		return settBank;
