				.selectSubMerTerminalByUpdatePwd(subMerTerminal);
		if (subMerTerminal != null) {
			subMerTerminal.setLoginPwd(selectPassWord.getPwd());
			setCheckCode("3042");
			int update = 0;
			try {
				update = subMerTerminalDao.updatePwd(subMerTerminal);
