package com.bypay.service.impl.util;
/**
 * @author zoulin
 * @version 创建时间：Nov 7, 2013 3:32:23 PM
 */

public class MerchantInfoBean {

    private String merType;//商户类型(2):01个人02公司
	private String merName;//商户名称(50)
	private String idNo;//身份证(30):当merType=01时必填
	private String busLic;//营业执照(50):当merType=02时必填
	private String taxCer;//税务登记证(50):当merType=02时必填
	private String orgCodeCer;//组织机构代码证(50):当merType=02时必填
	private String bankPermit;//银行开户许可证(50):当merType=02时必填
	private String bankAcc;//银行结算账号(20)
	private String tel;//联系电话(14)
	private String fee;//费率(6)
	private String agentId;//代理商编号(20)
	private String agentFee;//代理商费率(6)
	private String merSysId;//商户号(12)
	
	public String getMerType() {
		return merType;
	}
	public void setMerType(String merType) {
		this.merType = merType;
	}
	public String getMerName() {
		return merName;
	}
	public void setMerName(String merName) {
		this.merName = merName;
	}
	public String getIdNo() {
		return idNo;
	}
	public void setIdNo(String idNo) {
		this.idNo = idNo;
	}
	public String getBusLic() {
		return busLic;
	}
	public void setBusLic(String busLic) {
		this.busLic = busLic;
	}
	public String getTaxCer() {
		return taxCer;
	}
	public void setTaxCer(String taxCer) {
		this.taxCer = taxCer;
	}
	public String getOrgCodeCer() {
		return orgCodeCer;
	}
	public void setOrgCodeCer(String orgCodeCer) {
		this.orgCodeCer = orgCodeCer;
	}
	public String getBankPermit() {
		return bankPermit;
	}
	public void setBankPermit(String bankPermit) {
		this.bankPermit = bankPermit;
	}
	public String getBankAcc() {
		return bankAcc;
	}
	public void setBankAcc(String bankAcc) {
		this.bankAcc = bankAcc;
	}
	public String getTel() {
		return tel;
	}
	public void setTel(String tel) {
		this.tel = tel;
	}
	public String getFee() {
		return fee;
	}
	public void setFee(String fee) {
		this.fee = fee;
	}
	public String getAgentId() {
		return agentId;
	}
	public void setAgentId(String agentId) {
		this.agentId = agentId;
	}
	public String getAgentFee() {
		return agentFee;
	}
	public void setAgentFee(String agentFee) {
		this.agentFee = agentFee;
	}
	public String getMerSysId() {
		return merSysId;
	}
	public void setMerSysId(String merSysId) {
		this.merSysId = merSysId;
	}
}
